{
  "name": "datastore-sequences",
  "version": "1.0.8",
  "description": "Generate sequential counters for Google Clound Datastore.",
  "keywords": [
    "google api",
    "google cloud platform",
    "google cloud",
    "google datastore",
    "datastore"
  ],
  "repository": "https://github.com/mdornseif/datastore-sequences",
  "license": "MIT",
  "main": "build/main/index.js",
  "module": "build/module/index.js",
  "typings": "build/main/index.d.ts",
  "files": [
    "build/main",
    "build/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "test": "run-s build test:*",
    "test:lint": "eslint src --ext .ts",
    "test:spelling": "cspell \"{README.md,.github/*.md,src/**/*.ts}\"",
    "test:unit": "vitest src",
    "check-cli": "run-s test diff-integration-tests check-integration-tests",
    "check-integration-tests": "run-s check-integration-test:*",
    "diff-integration-tests": "mkdir -p diff && rm -rf diff/test && cp -r test diff/test && rm -rf diff/test/test-*/.git && cd diff && git init --quiet && git add -A && git commit --quiet --no-verify --allow-empty -m 'WIP' && echo '\\n\\nCommitted most recent integration test output in the \"diff\" directory. Review the changes with \"cd diff && git diff HEAD\" or your preferred git diff viewer.'",
    "watch:build": "tsc -p tsconfig.json -w",
    "watch:test": "vitest src/ --watch",
    "cov": "run-s build test:unit cov:html cov:lcov && open-cli coverage/index.html",
    "doc": "run-s doc:html && open-cli build/docs/index.html",
    "doc:html": "typedoc src/ --exclude **/*.spec.ts --out build/docs",
    "doc:json": "typedoc src/ --exclude **/*.spec.ts --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "version": "standard-version",
    "reset-hard": "git clean -dfx && git reset --hard && npm install",
    "prepare-release": "run-s reset-hard test cov:check doc:html version doc:publish",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*{css,scss,md,markdown,json,yaml,yml,graphql,html}": "npx prettier -w",
    "*{js,jsx,ts,tsx}": [
      "npx prettier -w",
      "npm run lint --fix"
    ],
    "package.json": "sort-package-json"
  },
  "prettier": {
    "printWidth": 110,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "release": {
    "branches": [
      "main",
      "master",
      "next"
    ]
  },
  "dependencies": {
    "assertate-debug": ">=2.4.2",
    "datastore-api": "^6.0.0",
    "debug": ">=4.3.3",
    "p-limit": "^3.1.0",
    "p-retry": "^4.6.1"
  },
  "devDependencies": {
    "@google-cloud/datastore": ">=6.0.0",
    "@types/node": ">=16.18.71",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "codecov": "^3.5.0",
    "cspell": "^4.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "gh-pages": "^3.1.0",
    "google-datastore-emulator": ">=5.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "open-cli": "^8.0.0",
    "prettier": "^2.1.1",
    "standard-version": "^9.0.0",
    "supports-color": "^9.2.1",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.10",
    "typescript": "<5.1.0",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "@google-cloud/datastore": ">=6.0.0"
  },
  "engines": {
    "node": ">=10"
  }
}
